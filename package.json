{
  "name": "nextjs-app",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "doppler run -- next dev -p 3004",
    "dev:local": "next dev -p 3004",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:ui": "playwright test --project=e2e --ui",
    "test:e2e:debug": "playwright test --project=e2e --debug",
    "test:e2e:headed": "playwright test --project=e2e --headed",
    "test:e2e:report": "playwright show-report",
    "test:setup:fixtures": "tsx tests/setup/generate-test-fixtures.ts",
    "test:smoke": "playwright test --project=smoke-chromium",
    "test:smoke:ui": "playwright test --project=smoke-chromium --ui",
    "test:smoke:debug": "playwright test --project=smoke-chromium --debug",
    "test:smoke:headed": "playwright test --project=smoke-chromium --headed",
    "test:all:playwright": "playwright test",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "knip:production": "knip --production",
    "validate": "npm run lint && npm run format:check && npm run typecheck && npm run test:unit && npm run test:smoke && npm run build",
    "pre-push": "./scripts/pre-push.sh",
    "pre-push:quick": "./scripts/pre-push.sh --skip-smoke",
    "ci": "./scripts/pre-push.sh"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.6",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@huggingface/transformers": "^3.7.6",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "file-type": "^21.0.0",
    "gray-matter": "^4.0.3",
    "next": "^15.5.9",
    "plyr": "^3.8.3",
    "plyr-react": "^5.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.0.0",
    "knip": "^5.70.1",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
